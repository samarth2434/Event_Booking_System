{
  "name": "event-booking-system",
  "version": "1.0.0",
  "description": "Complete Event Booking System with MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "seed": "node scripts/seedData.js",
    "setup": "node setup.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.4",
    "stripe": "^12.18.0",
    "nodemailer": "^6.9.4",
    "node-cron": "^3.0.2",
    "express-validator": "^7.0.1",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["event", "booking", "mern", "react", "mongodb"],
