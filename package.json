{
  "name": "event-booking-system",
  "version": "1.0.0",
  "description": "Complete Event Booking System with MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
